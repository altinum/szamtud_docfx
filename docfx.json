{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "globalMetadata": {
      "fileMetadata": {
        "conceptual": [
          "softec/index.md"
        ]
      },
      "breadcrumb_home": "softec/index.md"
    },
    "content": [
      {
        "files": [
          "softec/**.yml",
          "softec/**.md"
        ]
      },
      {
        "files": [
          "adatb/**.md",
          "adatb/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "softeng/**.yml",
          "softeng/**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "softec/**.zip",
          "softec/**.docx",
          "softec/**.txt",
          "softec/**.png",
          "softec/**.jpg",
          "softec/**.csv",
          "softeng/**.txt",
          "softeng/**.png",
          "softeng/**.jpg",
          "softeng/**.jpeg",
          "softeng/**.gif",
          "softeng/**.zip"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [

      "./_templates/default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "markdownEngineProperties": {
      //"markdigExtensions": [ "mathematics", "emoji" ]
    },

    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false

    //"markdown": {
    //  "extensions": [ "Markdig.Mathematics" ]
    //}


  }

}
