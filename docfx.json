{
  "metadata": [
    {
      "src": [
        "src/**.csproj"
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "softec/**.yml",
          "softec/**.md"
        ]
      },
      {
        "files": [
          "adatb/**.md",
          "adatb/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "softeng/**.yml",
          "softeng/**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "template": [
      {
        "default"
        "_templates/default"
      }
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
