{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "softec/**.yml",
          "softec/**.md"
        ]
      },
      {
        "files": [
          "adatb/**.md",
          "adatb/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
	{
        "files": [
          "softeng/**.yml",
          "softeng/**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "softec/**.zip",
          "softec/**.docx",
          "softec/**.txt"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      
	  "./_templates/default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
